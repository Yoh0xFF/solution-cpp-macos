{
  "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && clang++ -std=c++20 -stdlib=libc++ -g $fileName -o program.bin && $dirprogram.bin",
  },
  "clang.cxxflags": ["-std=c++20"],
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.associations": {
    "iostream": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "numeric": "cpp",
    "vector": "cpp",
    "set": "cpp",
    "unordered_set": "cpp",
    "map": "cpp",
    "unordered_map": "cpp",
    "stack": "cpp",
    "queue": "cpp",
    "string": "cpp",
    "any": "cpp",
  },
  "C_Cpp.errorSquiggles": "Disabled",
}